{
  "name": "@hg8496/umgfinder",
  "version": "2.0.2",
  "description": "A tool to find all Janitza UMGs in the specified network",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A lib",
    "postversion": "git push && git push --tags",
    "build": "tsc --build ",
    "format": "prettier --write \"lib/**/*.ts\" \"lib/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "author": "hg8496@cstolz.de",
  "license": "MIT",
  "dependencies": {
    "@hg8496/netmask": "^2.0.3"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/node": "^11.13.7",
    "prettier": "^1.17.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hg8496/umgfinder.git"
  },
  "keywords": [
    "janitza",
    "umg",
    "nodejs",
    "typescript"
  ],
  "bugs": {
    "url": "https://github.com/hg8496/umgfinder/issues"
  },
  "homepage": "https://github.com/hg8496/umgfinder#readme"
}
